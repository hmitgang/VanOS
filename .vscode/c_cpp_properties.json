{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        },
        {
            "name": "i686",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/libc/**",
            ],
            "defines": [],
            "macFrameworkPath": [
                "~/os/opt/cross/lib/gcc/i686-elf/12.2.0"
            ],
            "compilerPath": "~/os/opt/cross/bin/i686-elf-gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerArgs": [
                "-ffreestanding"
            ]
        }
    ],
    "version": 4
}